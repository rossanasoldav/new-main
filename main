class queryID(Enum):
    checkUSD        = 2350073
    checkETH        = 2350748
    checkBNBChain   = 2344893


threads = 1
pool    = Pool(threads)

query_res = {}
for q in queryID:
    query_res[q.name] = []

def round_to(num, digits=2):
    try:
        if num == 0: return 0
        scale = int(-math.floor(math.log10(abs(num - int(num))))) + digits - 1
        if scale < digits: scale = digits
        return round(num, scale)
    except: return num
